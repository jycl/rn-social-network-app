{
  "name": "SocialNetworkApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clear_all_caches": "watchman watch-del-all && rm -rf ./node_modules && rm -rf /tmp/metro-* && npm install && npm start -- --reset-cache"
  },
  "dependencies": {
    "mobx": "3.6.2",
    "mobx-react": "4.4.3",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-reanimated": "^1.9.0",
    "react-native-screens": "^2.5.0",
    "react-native-vector-icons": "^7.0.0",
    "react-navigation": "3.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.5.1",
    "metro-react-native-babel-preset": "^0.58.0",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "jest": "^24.9.0",
    "react-dom": "16.6.3",
    "react-test-renderer": "16.11.0"
  },
  "jest": {
    "preset": "react-native",
    "setupTestFrameworkScriptFile": "<rootDir>/src/config/setupTests.js",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!react-native|react-navigation)/"
    ]
  }
}
